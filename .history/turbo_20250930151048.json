{
  "$schema": "https://turborepo.com/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "cypress": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}
