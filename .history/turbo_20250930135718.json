{
  "$schema": "https://turborepo.com/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "cypress": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}